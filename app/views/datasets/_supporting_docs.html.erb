<section class="supporting docs">
  <h2 class="heading-medium">
    <%= t('datasets.show.supporting_docs')%>
  </h2>

  <table>
    <thead>
    <tr>
      <th class="title"><%= t('datasets.show.link_to_doc')%></th>
      <th><%= t('datasets.show.format')%></th>
      <th><%= t('datasets.show.date_added')%></th>
    </tr>
    </thead>
    <tbody>
    <% documents.each do |doc| %>
      <tr>
        <td class="title">
          <a href="<%= doc['url'] %>"><%= doc['name'] %></a>
        </td>
        <td><%= doc["format"] or "N/A" %></td>
        <td><%= doc["start_date"] or "N/A" %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</section>
